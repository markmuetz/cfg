# cp to .ssh/config
Host Puma
  HostName puma.nerc.ac.uk
  User markmuetz

Host JasminLogin
  HostName jasmin-login1.ceda.ac.uk
  User mmuetz
  ProxyCommand ssh -AY Puma -W %h:%p

Host JasminXfer
  HostName jasmin-xfer1.ceda.ac.uk
  User mmuetz
  ProxyCommand ssh -AY Puma -W %h:%p

Host jasmin-sci*.ceda.ac.uk
  User mmuetz
  ProxyCommand ssh -AY JasminLogin -W %h:%p
